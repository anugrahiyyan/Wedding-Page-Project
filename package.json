{
  "name": "wedding-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node src/scripts/run-strict.js next build",
    "start": "node server.js",
    "lint": "eslint",
    "db:setup": "npm run db:push && npm run db:seed",
    "db:push": "npx prisma db push",
    "db:seed": "node src/scripts/seed.js",
    "clean": "rm -rf .next",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sqlite3": "^5.1.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^5.22.0",
    "typescript": "^5"
  }
}